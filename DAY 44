#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n,a,b,c,d,e,f,g;
	    cin>>n>>a>>b>>c;
	    d=b-a;
	    e=b-c;
	    if(d>=c){
	        f=c;
	    }
	    else{
	        f=d;
	    }
	    if(e>=a){
	        g=a;
	    }
	    else{
	        g=e;
	    }
	    if((f+g)>=n){
	        cout<<"YES\n";
	    }
	    else{
	        cout<<"NO\n";
	    }
	}
	return 0;
}
