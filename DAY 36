#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int a,b,c,d,k;
	    cin>>a>>b>>c>>d>>k;
	    int x=abs(c-a);
	    int y=abs(d-b);
	    int z=x+y;
	    if(k>=z && (k-z)%2==0){
	        cout<<"Yes"<<endl;
	    }
	    else{
	        cout<<"No"<<endl;
	    }
	}
	return 0;
}
